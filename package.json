{
  "name": "linc-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node --inspect=5858 -r ts-node/register src/server/app.ts",
    "clean": "rimraf dist",
    "build:app": "next build src/app",
    "build:functions": "tsc",
    "copy:functions": "cpx \"functions/**/*.*\" dist/",
    "copy:static": "cpx \"src/app/static/**/*.*\" dist/public/static",
    "copy:next": "cpx \"dist/next/static/**/*.*\" dist/public/_next/static",
    "copy:deps": "cpx \"*{package.json,package-lock.json}\" dist",
    "build": "cross-env NODE_ENV=production run-s build:app build:functions copy:static copy:next copy:deps copy:functions ",
    "emulate": "npx @google-cloud/functions-framework --target=next --source=./ --port=3000",
    "lint": "eslint src/ --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "test": "jest"
  },
  "keywords": [
    "nextjs",
    "google functions"
  ],
  "author": "Pramendra Gupta <pramendra@mercari.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "next": "^9.3.2",
    "next-routes": "^1.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.4.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-eslint": "^10.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}
