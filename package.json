{
  "name": "linc-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next src/app",
    "clean": "rimraf dist",
    "build:app": "next build src/app",
    "build:functions": "cpx \"src/functions/**/*.*\" dist",
    "copy:static": "cpx \"src/app/static/**/*.*\" dist/public/static",
    "copy:next": "cpx \"dist/next/static/**/*.*\" dist/public/_next/static",
    "copy:deps": "cpx \"*{package.json,package-lock.json}\" dist",
    "copy:pages": "cpx \"dist/next/serverless/pages/**/*.*\" dist/pages",
    "build": "cross-env NODE_ENV=production run-s clean build:app build:functions copy:static copy:next copy:deps copy:pages",
    "emulate": "cross-env NODE_ENV=production functions-framework --target=next --source=./dist --port=3000",
    "lint": "eslint src/ --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nextjs",
    "google functions"
  ],
  "author": "Pramendra Gupta <pramendra@mercari.com>",
  "license": "ISC",
  "dependencies": {
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "next": "^9.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.4.0",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-eslint": "^10.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.2"
  }
}
